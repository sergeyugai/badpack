{
    "name": "sergeyugai/badpack",
    "type": "library",
    "description": "Admin panel builder based on Backpack for Laravel but with different API",
    "keywords": [
        "laravel",
        "backpack"
    ],
    "homepage": "https://github.com/sergeyugai/badpack",
    "license": "MIT",
    "authors": [
        {
            "name": "Sergey Yugai",
            "email": "sergey@sergeyugai.com",
            "homepage": "https://sergeyugai.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4",
        "backpack/crud": "4.1.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^8",
        "orchestra/testbench": "^5.3"
    },
    "autoload": {
        "psr-4": {
            "SergeYugai\\Laravel\\BadPack\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SergeYugai\\Laravel\\BadPack\\": "src",
            "Backpack\\CRUD\\Tests\\": "vendor/backpack/crud/tests",
            "Tests\\": "tests"
        },
        "files": ["vendor/backpack/crud/src/helpers.php"]
    }
}
